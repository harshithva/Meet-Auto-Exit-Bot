{
    "name": "Meet Auto Exit Bot",
    "description": "Exits meet based on number of people present.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "128": "icon_128.png"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ]
}